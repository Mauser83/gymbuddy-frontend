{
  "name": "frontend",
  "slug": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start -c",
    "startcvdev": "cross-env APP_ENV=cvdev expo start -c",
    "buildios": "eas build -p ios --profile production",
    "submitios": "eas submit -p ios --profile production --latest",
    "buildcvdev": "eas build -p ios --profile cvdev",
    "submitcvdev": "cross-env APP_ENV=cvdev eas submit -p ios --profile cvdev --latest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ci": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@apollo/client": "^3.13.7",
    "@babel/eslint-parser": "^7.27.0",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/native": "^6.1.7",
    "@shopify/restyle": "^2.4.5",
    "date-fns": "^4.1.0",
    "expo": "^54.0.10",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "formik": "^2.4.6",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "graphql-ws": "^6.0.4",
    "i18n-iso-countries": "^7.14.0",
    "jwt-decode": "^4.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-network-info": "^5.2.1",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "react-router-native": "^6.30.0",
    "styled-components": "^6.1.17",
    "subscriptions-transport-ws": "^0.11.0",
    "volkeno-react-native-drag-drop": "^1.1.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tsconfig/react-native": "^3.0.6",
    "@types/date-fns": "^2.6.3",
    "@types/jest": "^30.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-network-info",
          "react-native-vector-icons"
        ]
      }
    }
  },
  "private": true
}
