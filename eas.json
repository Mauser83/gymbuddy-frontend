{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "APP_ENV": "production"
      },
      "ios": {"simulator": false}
    },
    "cvdev": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "APP_ENV": "cvdev"
      },
      "ios": {"simulator": false}
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "mauser83@hotmail.com",
        "ascAppId": "6749501544"
      }
    },
    "cvdev": {
      "ios": {
        "appleId": "mauser83@hotmail.com",
        "ascAppId": "6749850841"
      }
    }
  }
}
